generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model course {
  id                 Int     @id @default(autoincrement())
  name               String  @db.VarChar(100)
  description        String
  learning_objective Json
  prerequisite       String?
  conclusion         String
}

model user {
  id    Int    @id @default(autoincrement())
  name  String @db.VarChar(100)
  email String @unique @db.VarChar(100)
}
